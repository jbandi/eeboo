{
  "name": "analyzeyou",
  "version": "0.0.0",
  "engines": {
    "node": ">=8.0.0"
  },
  "eslintConfig": {
    "extends": [
      ".eslintrc-node"
    ]
  },
  "private": true,
  "scripts": {
    "start": "NODE_ENV=prod node server.js",
    "start-dev": "NODE_ENV=dev node server.js",
    "start-test": "NODE_ENV=test node server.js",
    "test": "mocha --timeout 10000 --exit",
    "test-coverage": "nyc --reporter=html mocha --timeout 10000",
    "heroku-postbuild": "cd web && yarn --production=false && yarn run build",
    "lint": "eslint --ext .js *.js controllers/ test/",
    "lint-fix": "eslint --ext .js --fix *.js controllers/ test/"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "config": "^1.27.0",
    "cookie-parser": "~1.4.3",
    "dateformat": "^3.0.2",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "express-jwt": "^5.3.0",
    "express-jwt-authz": "^1.0.0",
    "firebase": "^4.6.0",
    "firebase-admin": "^5.4.2",
    "idx": "^2.2.0",
    "jade": "~1.11.0",
    "jwks-rsa": "^1.2.0",
    "morgan": "~1.8.1",
    "serve-favicon": "~2.4.2",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-chai": "^0.0.1",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1"
  }
}
