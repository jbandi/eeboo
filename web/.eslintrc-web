{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "root": true,
  "plugins": [
    "import",
    "jsx-a11y",
    "react"
  ],
  "rules": {
    "strict": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-undef": "off",
    "jsx-a11y/anchor-is-valid": "off",
  },
}
